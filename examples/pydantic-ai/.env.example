# Mapbox API token (for MCP server - server-side use)
# Get your token at https://account.mapbox.com/
MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# Mapbox PUBLIC token (for interactive map - browser use)
# Required for warsaw_gradio_blocks_interactive.py
# Must be a PUBLIC token starting with 'pk.'
# Get one at https://account.mapbox.com/access-tokens/
MAPBOX_PUBLIC_TOKEN=pk.your_public_token_here

# OpenAI API key (if using OpenAI models)
OPENAI_API_KEY=your_openai_key_here

# Anthropic API key (if using Claude models)
ANTHROPIC_API_KEY=your_anthropic_key_here
